1.
SELECT TITLE, COST, RETAIL, CATEGORY
FROM BOOKS
WHERE COST BETWEEN 35 and 60
ORDER BY TITLE;

2. 
SELECT TITLE, ISBN
FROM BOOKS
WHERE CATEGORY IN ('COMPUTER');

3.
SELECT LASTNAME, FIRSTNAME, ADDRESS, CITY, STATE, ZIP
FROM CUSTOMERS
WHERE LASTNAME LIKE ('L%') ;

4. 
SELECT TITLE
FROM BOOKS
WHERE CATEGORY IN ('FAMILY LIFE', 'FITNESS');
5. 

SELECT ORDER#, ORDERDATE
FROM ORDERS
WHERE SHIPDATE IS NOT NULL
ORDER BY ORDERDATE
